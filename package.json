{
  "homepage": "https://rodriguezcodesolutions.tech",
  "name": "devslanding",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/eslint-parser": "^7.26.10",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.8.3",
    "lucide-react": "^0.525.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-scripts": "^5.0.1",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "env-update": "cross-env REACT_APP_GIT_COMMIT_HASH=$(git rev-parse --short HEAD) REACT_APP_LAST_UPDATE=$(date +%Y-%m-%d) && node -e \"require('fs').writeFileSync('.env', `REACT_APP_GIT_COMMIT_HASH=${process.env.REACT_APP_GIT_COMMIT_HASH}\\nREACT_APP_LAST_UPDATE=${process.env.REACT_APP_LAST_UPDATE}\\n`)\"",
    "build": "npm run env-update && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
  ,
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "gh-pages": "^6.3.0"
  }
}
